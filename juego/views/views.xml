<odoo>
  <data>

    <record model="ir.actions.act_window" id="juego.action_jugador">
      <field name="name">jugador window</field>
      <field name="res_model">juego.jugador</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="juego.action_planeta">
      <field name="name">planeta window</field>
      <field name="res_model">juego.planeta</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="juego.action_edificio">
      <field name="name">edificio window</field>
      <field name="res_model">juego.edificio</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="juego.action_edificio_tipo">
      <field name="name">edificio_tipo window</field>
      <field name="res_model">juego.edificio_tipo</field>
      <field name="view_mode">tree,form</field>
    </record>


    <menuitem name="Juego" id="juego.menu_root"/>

    <menuitem name="Menu 1" id="juego.menu_management" parent="juego.menu_root"/>

    <menuitem name="Jugador" id="juego.menu_1_jugador" parent="juego.menu_management"
              action="juego.action_jugador"/>

    <menuitem name="Planeta" id="juego.menu_1_planeta" parent="juego.menu_management"
              action="juego.action_planeta"/>

    <menuitem name="Edificio" id="juego.menu_1_edificio" parent="juego.menu_management"
              action="juego.action_edificio"/>

    <menuitem name="Tipo Edificio" id="juego.menu_1_edificio_tipo" parent="juego.menu_management"
              action="juego.action_edificio_tipo"/>

  </data>
</odoo>